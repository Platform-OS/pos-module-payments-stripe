{%- liquid
  if account_id == blank
    log "account_id is null", type: "QUERY ERROR"
    return null
  endif

  graphql g  = 'modules/payments_stripe/connected_accounts/search', account_id: account_id, limit: 1
  if g.records == blank
    log account_id, type: "QUERY ERROR - PARAMS"
    log g, type: "QUERY ERROR"
  endif

  return g.records.results[0]
-%}
