{%- liquid
  if customer_id == blank
    log "customer_id is null", type: "QUERY ERROR"
    return null
  endif

  graphql g  = 'modules/payments_stripe/customers/search', customer_id: customer_id, limit: 1
  if g.records == blank
    log customer_id, type: "QUERY ERROR - PARAMS"
    log g, type: "QUERY ERROR"
  endif

  return g.records.results[0]
-%}
