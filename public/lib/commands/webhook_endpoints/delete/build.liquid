{% liquid
  assign body = response.body | parse_json
%}
{% parse_json webhook %}
  {%- if response.status == 200 -%}
    {
      {%- if body.error -%}
        "errors": [
          {"message": "{{ body.error.message }}"}
        ],
      {%- endif -%}
      "gateway_id": "{{ body.id }}",
      "valid": true
    }
  {%- else -%}
    {
      "errors": {{ body.error.message | json }},
      "valid": false
    }
  {%- endif -%}
{% endparse_json %}
{% liquid
  return webhook
%}
